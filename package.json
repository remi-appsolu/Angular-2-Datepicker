    "material"
  ],
  "devDependencies": {
    "@angular/animations": "^7.2.9",
    "@angular/common": "^7.2.9",
    "@angular/compiler": "^7.2.9",
    "@angular/compiler-cli": "^7.2.9",
    "@angular/core": "^7.2.9",
    "@angular/forms": "^4.0.1",
    "@angular/platform-browser": "^7.2.9",
    "@angular/platform-browser-dynamic": "^7.2.9",
    "@angular/platform-server": "^4.0.1",
    "@types/jasmine": "^2.5.38",
    "concurrently": "^3.0.0",
    "core-js": "^2.4.1",
    "es6-shim": "^0.35.1",
    "jasmine-core": "~2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "moment": "^2.17.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^6.4.0",
    "systemjs": "0.19.39",
    "tslint": "^5.14.0",
    "typescript": "^3.2.4",
    "zone.js": "^0.8.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jlampton/Angular-2-Datepicker"
  },
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "lint": "tslint ./src/datepicker.*.ts -t verbose",
    "lite": "lite-server",
    "test": "tsc && karma start karma.conf.js",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "ngc": "node_modules/.bin/ngc -p tsconfig-aot.json && rm integration-test/*.metadata.json",
    "build": "rm -rf lib && ngc -p tsconfig-build.json",
    "build:aot": "rm -rf lib && ngc -p tsconfig-aot.json"
  },
  "dependencies": {
    "@angular/cdk": "^7.3.4",
    "@angular/material": "^6.4.7",
    "rxjs-compat": "^6.4.0"
  }
